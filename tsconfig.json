{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    // "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"],
      "@/assets/*": [
        "assets/*"
      ],
      "@/config/*": [
        "config/*"
      ],
      "@/features/*": [
        "features/*"
      ],
      "@/providers/*": [
        "providers/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/generated/*": [
        "generated/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/type/*": [
        "type/*"
      ],
    }
  }
}
